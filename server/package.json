    "google-auth-library": "^8.7.0",
    "isolated-vm": "^4.4.2",
    "node-postgres": "^0.6.2",
    "pg": "^8.8.0",
    "request": "^2.88.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.10",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
