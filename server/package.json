  "main": "./dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "fuser -k 8080/tcp & nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "dev:start": "npm-run-all build start",
    "lint": "npx prettier --write src && npx eslint src --fix",
    "start": "node ./dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "npm version patch && npm shrinkwrap && zip -r artifact-${npm_package_version}.zip ./ -x '*.git' -x '*artifact*zip*' -x '*artifact.zip*' -x '*node_modules*'"
  },
  "author": "",
